#: venv/lib/python2.7/site-packages/django/contrib/postgres/apps.py:12
msgid "PostgreSQL extensions"
msgstr "Extensões de PostgresSQL"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/array.py:18
#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/array.py:154
#, python-format
msgid "Item %(nth)s in the array did not validate: "
msgstr "Item %(nth)s na lista não validou:"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "As sub-listas têm de ter o mesmo tamanho."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/hstore.py:16
msgid "Map of strings to strings"
msgstr "Mapeamento de strings em strings."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/hstore.py:18
#, python-format
msgid "The value of \"%(key)s\" is not a string."
msgstr "O valor de \"%(key)s\" não é uma string."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/jsonb.py:15
msgid "A JSON object"
msgstr "Um objecto JSON"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/fields/jsonb.py:17
msgid "Value must be valid JSON."
msgstr "O valor deve ser JSON válido"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Não foi possível carregar os dados JSON."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr ""

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/jsonb.py:16
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "O valor '%(value)s' deve ser JSON válido."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:13
msgid "Enter two valid values."
msgstr "Introduza dois valores válidos."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:14
msgid "The start of the range must not exceed the end of the range."
msgstr "O início da gama não pode ser maior que o seu fim."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:61
msgid "Enter two whole numbers."
msgstr "Introduza dois números inteiros."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:67
msgid "Enter two numbers."
msgstr "Introduza dois números."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:73
msgid "Enter two valid date/times."
msgstr "Introduza duas datas/horas válidas."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/forms/ranges.py:79
msgid "Enter two valid dates."
msgstr "Introduza duas datas válidas."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:14
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, não pode conter mais do que "
"%(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, não pode conter mais do que "
"%(limit_value)d."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:21
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"A lista contém %(show_value)d item, tem de conter pelo menos %(limit_value)d."
msgstr[1] ""
"A lista contém %(show_value)d itens, tem de conter pelo menos "
"%(limit_value)d."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:31
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Algumas chaves estão em falta: %(keys)s"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:32
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Foram fornecidas algumas chaves desconhecidas: %(keys)s"

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "Garanta que esta gama é toda ela menor ou igual a %(limit_value)s."

#: venv/lib/python2.7/site-packages/django/contrib/postgres/validators.py:82
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "Garanta que esta gama é toda ela maior ou igual a %(limit_value)s."
